                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import socket
import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.file_path = ''

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):

        self.file_path = ''
        self.sid = ''
        self.recording_time = ''

        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self, id):
        #self.onStopped() #activate the output of the box
        self.sid = id.lower()
        self.recording_time = time.strftime(" %Y-%m-%d %H\'%M", time.localtime())

    def onInput_file_path_get(self, file_path):
        self.file_path = file_path
        self.say('請問要傳送檔案嗎? 如果要傳送請說開始傳送，要重新錄音則說重新錄音')
        self.make_decisions()

    def onInput_transfer(self, decision):
        port = 5007
        SIZE = 1024

        if decision == '重新錄音':
            self.say('要錄音時請再摸我頭的前面')
            self.re_record()
        elif decision == '開始傳送':
            try:
                s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
                s.connect(("140.134.26.200", port))

                s.send('file')
                print s.recv(SIZE)

                file_name = self.sid + ';' + self.sid + self.recording_time + '-record.wav'

                self.logger.info(file_name)
                s.send(file_name)
                print s.recv(SIZE)

                self.say('傳送中，這需要一點時間，大約一到兩分鐘')
                self.logger.info('sending, please wait for a second ...')
                time.sleep(0.2)
                with open(self.file_path, 'rb') as f:
                    for data in f:
                        s.send(data)

                self.logger.info('sended !')
                self.logger.info('connection closed')

                self.say('傳送完畢')
            except:
                self.say('伺服器出了點狀況，麻煩檢查伺服器')


            finally:
                try:
                    s.close()
                    self.onUnload()
                    self.onStopped()
                except:
                    pass

        else:
            self.say('我出了點問題，請重新錄音')
            self.re_record()

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
