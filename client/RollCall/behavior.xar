                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.allnames = []
        self.rollcall = ''
        self.name = ''
    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self, p):
        self.bIsRunning = True
        self.allnames = p.split(';')
        if self.allnames[len(self.allnames)-1] == '':
            del self.allnames[len(self.allnames)-1]
        try:
            if len(self.allnames) > 0:
                self.name = self.allnames[0].split()
                sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
                sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
                sentence += str('開始點名, 第一個是' + self.name[1])
                sentence +=  "\RST\ "
                configuration =\
                {"bodyLanguageMode":self.getParameter("Body language mode")}
                id = self.animSpeech.post.say(str(sentence), configuration)
                self.ids.append(id)
                self.animSpeech.wait(id, 0)
            else:
                self.onStopped(self.rollcall)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
    def onInput_haveCome(self):
        try:
            sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
            sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
            sentence += str(self.name[1] + '有來')
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":self.getParameter("Body language mode")}
            id = self.animSpeech.post.say(str(sentence), configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
        self.rollcall += self.allnames[0] + ' 出席;'
        del self.allnames[0]
        try:
            if len(self.allnames) > 0:
                self.name = self.allnames[0].split()
                sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
                sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
                sentence += str('下一位是' + self.name[1])
                sentence +=  "\RST\ "
                configuration =\
                {"bodyLanguageMode":self.getParameter("Body language mode")}
                id = self.animSpeech.post.say(str(sentence), configuration)
                self.ids.append(id)
                self.animSpeech.wait(id, 0)

            else:
                sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
                sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
                sentence += str('點名完畢')
                sentence +=  "\RST\ "
                configuration =\
                {"bodyLanguageMode":self.getParameter("Body language mode")}
                id = self.animSpeech.post.say(str(sentence), configuration)
                self.ids.append(id)
                self.animSpeech.wait(id, 0)
                self.onStopped(self.rollcall)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
    def onInput_notCome(self):
        try:
            sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
            sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
            sentence += str(self.name[1] + '沒有來')
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":self.getParameter("Body language mode")}
            id = self.animSpeech.post.say(str(sentence), configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
        self.rollcall += self.allnames[0] + ' 曠課;'
        del self.allnames[0]
        try:
            if len(self.allnames) > 0:
                self.name = self.allnames[0].split()
                sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
                sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
                sentence += str('下一位是' + self.name[1])
                sentence +=  "\RST\ "
                configuration =\
                {"bodyLanguageMode":self.getParameter("Body language mode")}
                id = self.animSpeech.post.say(str(sentence), configuration)
                self.ids.append(id)
                self.animSpeech.wait(id, 0)
            else:
                sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
                sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
                sentence += str('點名完畢')
                sentence +=  "\RST\ "
                configuration =\
                {"bodyLanguageMode":self.getParameter("Body language mode")}
                id = self.animSpeech.post.say(str(sentence), configuration)
                self.ids.append(id)
                self.animSpeech.wait(id, 0)
                self.onStopped(self.rollcall)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
    def onInput_say_again(self):
        try:
            sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
            sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
            sentence += str('我再說一次' + self.name[1])
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":self.getParameter("Body language mode")}
            id = self.animSpeech.post.say(str(sentence), configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)

        finally:
            try:
                self.ids.remove(id)
            except:
                pass
    def onInput_onStop(self):
        self.onUnload()]]>
</content>
