                                                                    <script language="4">
                                                                        <content>
                                                                            <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.allnames = []
        self.rollcall = ''
        self.name = ''
        self.lock = True
    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self, p):
        self.bIsRunning = True
        self.allnames = p.split(';')
        if self.allnames[len(self.allnames)-1] == '':
            del self.allnames[len(self.allnames)-1]

        if len(self.allnames) > 0:
            self.name = self.allnames[0].split()
            self.say('開始點名, 第一個是' + self.name[1])
        else:
            self.onStopped(self.rollcall)

    def onInput_haveCome(self):
        if self.lock == False:
            pass
        else:
            self.lock = False
            self.say(self.name[1] + '有來')
            self.rollcall += self.allnames[0] + ' 出席;'
            del self.allnames[0]
            if len(self.allnames) > 0:
                self.name = self.allnames[0].split()
                self.say('下一位是' + self.name[1])
            else:
                self.say('點名完畢')
                self.onStopped(self.rollcall)
            self.lock = True

    def onInput_notCome(self):
        if self.lock == False:
            pass
        else:
            self.lock = False
            self.say(self.name[1] + '沒有來')
            self.rollcall += self.allnames[0] + ' 曠課;'
            del self.allnames[0]

            if len(self.allnames) > 0:
                self.name = self.allnames[0].split()
                self.say('下一位是' + self.name[1])
            else:
                self.say('點名完畢')
                self.onStopped(self.rollcall)
            self.lock = True

    def onInput_say_again(self):
        if self.lock == False:
            pass
        else:
            self.lock = False
            self.say('我再說一次' + self.name[1])
            self.lock = True

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":self.getParameter("Body language mode")}
            id = self.animSpeech.post.say(str(sentence),configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped() #activate the output of the box]]>
</content>
                                                                    </script>
