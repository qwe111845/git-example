                                                        <![CDATA[# -*- coding: UTF-8 -*-
#!/usr/bin/env python

import speech_recognition as sr
import socket


class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.executing = True

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        self.executing = False
        self.outputError("Sorry, I spaced out. Can you say that again? ,Please touch my front head, and say your answer")
        pass

    def onInput_wavPath(self, p):
        self.executing = True
        import wave
        WAV_FILE = p #get wav file path


        f = wave.open(WAV_FILE, 'rb')

        params = f.getparams()
        nchannels, sampwidth, framerate, nframes = params[:4]
        time = nframes / (1.0 * framerate)
        f.close()
        r = sr.Recognizer()
        with sr.WavFile(WAV_FILE) as source:
            audio = r.record(source) # read the entire WAV

        try:
            response = (r.recognize_google(audio,language="en-US")).encode('utf-8')
            if self.executing:
                self.outputString([str(response),time])
        except sr.UnknownValueError:
            self.outputError("Can you repeat that? I couldn't understand you. Please touch my front head, and say your answer", 80)
        except sr.RequestError as e:
            self.outputError("I can't reach google! The internet must be broken! Please touch my front head, and say your answer", 80)
        except sr.URLError as e:
            self.outputError("Something went wrong. Idk what it is but though. ,Please touch my front head, and say your answer", 80)
        except socket.timeout:
            self.outputError("One of my sockets timed out!")
        pass

    def onInput_stop(self):
        self.onUnload()]]>
</content>
