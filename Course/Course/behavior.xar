                                <content>
                                    <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.change = False

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self):
        #self.say("Please login to the account first", 80)
        self.change = True
        #self.say('Please put QR code in front of me', 80)
        self.say('請老師先登入您的帳號', 90)
        self.say('請把 QR code 放在我的眼睛前面', 90)

    def onInput_finish_word_practice(self):
        #self.say('Start reading exercises now', 80)
        self.say('開始閱讀練習', 90)
        self.startReading('lesson' + self.lesson)

    def onInput_finish_reading(self):
        #self.say('Please touch my front head to start recording')
        self.say('課程練習完畢，請摸我頭的前面開始做閱讀測驗', 90)
        self.startReadingTest([self.getParameter('id'), str(self.lesson)])

    def onInput_finish_reading_test(self):
        #self.say('Please touch my front head to start recording')
        self.say('課程學習完畢，老師如果要練習下一單元請說練習，要離開則說離開', 90)
        self.finish_all()

    def onInput_login(self, p):

        if self.change:
            self.change = False
            id = p[0][0]
            #self.say('Current id is ' + self.getParameter('id'), 80)
            #self.say('Connect to server', 80)
            self.logger.info(id)
            self.say('目前的帳號是 ' + self.getParameter('id'), 90)
            self.say('連接伺服器中，請稍等', 90)
            try:
                port = 5007
                s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
                s.connect(('140.134.26.200', port))
                s.send('teacher account')
                s.recv(1024)

                time.sleep(0.5)
                s.send(id)
                answer = s.recv(1024)
                lessons = answer.split(';')

                self.logger.info(lessons)
                self.lesson_id = lessons[0]
                self.lesson = lessons[1]
                self.title = lessons[2]

                if answer == 'no account':
                    #self.say('I don\'t have the ID information, please re-scan QR code', 80)
                    self.say('找不到這個帳號的資料喔，請重新掃描 QR code', 90)
                    self.change = True

                else:
                    #self.say('change to ' + id, 80)
                    self.setParameter('id', id)
                    #self.say('start learning lesson ' + self.lesson + ' ' + self.title, 80)
                    self.say('帳號 ' + str(id) + ' 設定成功，蔡明秀老師您好', 90)
                    self.say('請問要點名嗎?')
                    self.ask_for_rollcall()
            except:
                self.say('伺服器有問題，請檢查伺服器，檢查完請在掃描 QR code')
                self.change = True

    def onInput_finish_rollcall(self):
        self.say('請摸我頭的前面來開始學習單字')
        self.waiting_for_word()

    def onInput_rollcall_answer(self, answer):
        if answer == '好':
            self.start_rollcall(self.getParameter('id'))
        elif answer == '不要':
            self.sing()
            #self.say('好的那請摸我的頭開始單字練習')
            #self.waiting_for_word()

    def onInput_word_practice(self):
        self.say('開始學習單元 ' + self.lesson_id + ' ' + self.title, 90)
        self.say('首先是單字的練習，大約十秒後開始練習', 90)
        self.word(self.lesson_id)

    def onInput_course_selection(self, command):
        port = 5007
        if command == '練習':
            self.say('好的等我記錄一下課程，開始連接伺服器', 90)
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('update progress course')
            s.recv(1024)
            if int(self.lesson_id) < 9:
                id = int(self.lesson_id) + 1
            data = self.getParameter('id') + ';' + str(id)
            s.send(data)
            s.close()

            time.sleep(1)
            self.say('課程記錄成功，請稍等', 90)

            ss = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            ss.connect(('140.134.26.200', port))
            ss.send('teacher account')
            ss.recv(1024)
            ids = self.getParameter('id')
            ss.send(ids)
            answer = ss.recv(1024)
            lessons = answer.split(';')
            self.logger.info(lessons)
            self.lesson_id = lessons[0]
            self.lesson = lessons[1]
            self.title = lessons[2]
            ss.close()

            self.say('請老師摸我的前頭開始練習單字', 90)
            self.waiting_for_word()

        elif command == '離開':
            self.say('等我記錄一下課程，開始連接伺服器', 90)
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('update progress course')
            s.recv(1024)
            if int(self.lesson_id) < 9:
                id = int(self.lesson_id) + 1
            data = self.getParameter('id') + ';' + str(id)
            s.send(data)
            s.close()

            self.say('紀錄完畢，下次再見，掰掰', 90)
            self.setParameter('id', '000')
            self.lesson_id = ''
            self.lesson = ''
            self.title = ''
            self.change = True

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
