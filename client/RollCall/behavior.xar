                                                        <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.class_names = ''

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self):

        port = 5007

        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.connect(('140.134.26.200', port))

        self.say('抓取資料中')
        s.send('所有課程')

        class_data = s.recv(2048)

        self.class_names = class_data.split(';')
        if self.class_names[len(self.class_names)-1] == '':
            del self.class_names[len(self.class_names)-1]

        self.say('完成')

        self.onStopped() #activate the output of the box

    def onInput_Comparison(self, p):

        have_class = False
        for i in self.class_names:
            if p == i:
                have_class = True
                self.say('找到課程')
                self.get_class(p)

        if have_class == False:
            self.Course_not_found('查無此課程 麻煩再說一遍')


    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
