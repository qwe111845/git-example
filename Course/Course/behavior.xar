                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import socket
import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.quiz_data = {}
        self.sid = ''
        self.order = 0
        self.answer = ''
        self.stu_answer = ''
        self.reading_speed = 75
    def onLoad(self):
        #put initialization code here
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        #put clean-up code here
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self,p):
        #self.onStopped() #activate the output of the box
        import json
        port = 5007
        self.sid = p[0]
        try:
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('quiz_course')
            response = s.recv(2048)
            s.send(str(p[1]))
            time.sleep(0.5)
            data = s.recv(4096)
            self.logger.info(data)
            self.quiz_data = json.loads(data)

            s.close()
            self.wait_for_teacher()
        except socket.error, msg:
            self.logger.info(msg)
            self.say('server error, please check the server first', self.reading_speed)

    def onInput_reading_test(self):

        if self.order > 3:
            self.order = 0
            self.say('End of reading comprehension', self.reading_speed)
            self.waiting()
        elif self.order <= 3:
            self.say('Question ' + str(self.quiz_data['order'][self.order]), self.reading_speed)
            time.sleep(0.5)
            self.say(str(self.quiz_data['quiz'][self.order]), self.reading_speed)
            time.sleep(2)
            self.say(str(self.quiz_data['quiz'][self.order]), self.reading_speed)
            self.order +=1
            self.wait_for_teacher()

    def startReading_test(self):
        self.say('Question ' + str(self.quiz_data['order'][self.order]), self.reading_speed)
        self.say(str(self.quiz_data['quiz'][self.order]), self.reading_speed)
        time.sleep(1)
        self.say('Which answer do you select?', self.reading_speed)
        self.choose()

    def onInput_get_answer(self, answer):
        self.answer = answer
        self.say('OK! Please touch my front head and say your answer', self.reading_speed)
        self.record_answer()

    def onInput_next_question(self):
        self.startReading_test()

    def onInput_student_answer(self, stu_answer):
        self.stu_answer = stu_answer[0]
        word_len = len(self.stu_answer.split(' '))
        #self.logger.info(self.stu_answer)
        self.time = stu_answer[1]
        #self.logger.info(self.time)
        self.speed =  int((word_len / (1.0 * self.time)) * 60.0)
        #self.logger.info(self.speed)
        #self.logger.info(str(self.quiz_data['content'][self.order]))
        truth = self.quiz_data['content'][self.order].encode('utf-8')
        wer = self.get_wer(truth, self.stu_answer)
        #self.logger.info(self.error_rate)

        self.say('Your answer is ' + self.stu_answer, self.reading_speed)
        time.sleep(1)
        #' + self.error_rate + '
        self.say('The correction rate of this conversation is, ' + str(wer) + ' percent, and the speed is '+ str(self.speed) + ' words per menit.', self.reading_speed)



        if self.answer == self.quiz_data['answer'][self.order]:
            self.say('You are right! Thank you for your answer.', self.reading_speed)
        else:
            self.say('You are wrong! Thank you for your answer.', self.reading_speed)
        if self.order < 3:
            self.order +=1
            self.waiting()
        else:
            self.order = 0
            self.quiz_data = {}
            self.sid = ''
            self.answer = ''
            self.stu_answer = ''
            self.say('The test is over', self.reading_speed)
            self.finish_Reading_test()


    def get_wer(self, real, say):
        port = 5007
        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.connect(('140.134.26.200', port))
        s.send('wer')
        response = s.recv(2048)
        s.send(str(real) + ';;' + say)
        time.sleep(0.5)
        wer = s.recv(4096)
        s.close()

        return wer

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.finish_Reading_test()]]>
</content>
