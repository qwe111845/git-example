    def onInput_onGet(self,p):
        #self.onStopped() #activate the output of the box
        port = 5007
        try:
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('找課程')
            isok = s.recv(2048)
            currentTime = d.datetime.now()
            course_data = p + ',' + str(d.datetime.isoweekday(currentTime)) \
                          + ',' +  currentTime.strftime('%H')
            self.logger.info(isok)
            self.logger.info(course_data)
            if isok == 'ok':
                s.send(course_data)

            class_name = s.recv(2048)


        finally:
            try:
                s.close()
                self.onStopped(class_name) # activate output of the box
            except:
                pass



    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
