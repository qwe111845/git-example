                                                        <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.paragraph = 1
        self.end = False

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []
        self.framemanager = ALProxy("ALFrameManager")

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self, unit):
        import os
        self.end = False
        self.unit = unit
        self.path = '/var/persistent/home/nao/Audiofile/' + self.unit + '/'
        file = os.listdir(self.path)
        self.paragraph = 1
        self.paragraphs = len(file)
        self.logger.info(file)
        self.say('現在開始播放音檔，要播放下一個請按我的右手，重複練習請摸我頭的前面', 90)
        self.onStopped(self.path + self.unit + '-' + str(self.paragraph) + '.mp3')

    def onInput_add_paragraph(self):
        self.paragraph += 1
        if self.paragraph <= self.paragraphs:
            self.onStopped(self.path + self.unit + '-' + str(self.paragraph) + '.mp3')
        else:
            self.end_reading()

    def onInput_repeat(self):
        if self.paragraph <= self.paragraphs:
            self.onStopped(self.path + self.unit + '-' + str(self.paragraph) + '.mp3')
        else:
            self.end_reading()

    def end_reading(self):
        #self.say('At the end of the reading exercise, you can touch my left hand to leave, or touch my head start reading exercises again.', 80)
        self.say('閱讀練習結束了，如果要重新練習請摸我的頭中間，離開閱讀練習請按我的左手', 90)
        self.end = True
        self.finish_reading()

    def onInput_reStart(self):
        import os
        self.end = False
        self.paragraph = 1

        self.path = '/var/persistent/home/nao/Audiofile/' + self.unit + '/'
        file = os.listdir(self.path)

        self.paragraphs = len(file)
        self.logger.info(file)
        self.onStopped(self.path + self.unit + '-' + str(self.paragraph) + '.mp3')

    def say(self, p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":"contextual"}
            id = self.animSpeech.post.say(str(sentence),configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass

    def onInput_onStop(self):
        if self.end:
            self.say('離開閱讀練習', 80)
            self.paragraph = 1
            self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
            self.out_reading() #activate the output of the box
        else:
            self.finish_reading()]]>
</content>
