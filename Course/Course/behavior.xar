                                                        <![CDATA[import time
import datetime as d
import socket

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.animSpeech = ALProxy('ALAnimatedSpeech')
        self.class_names = ''
        self.change = False

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.animSpeech.stop(id)
            except:
                pass

    def onInput_onStart(self, teacher_name):
        self.say('點名帳號' + teacher_name)
        if teacher_name[0] != 't':
            self.say('不是老師不可點名')
            self.error()
        else:
            port = 5007
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('找課程')
            isok = s.recv(2048)

            currentTime = d.datetime.now()
            course_data = teacher_name + ',' + str(d.datetime.isoweekday(currentTime)) \
                          + ',' +  currentTime.strftime('%H')

            self.logger.info(course_data)

            if isok == 'ok':
                s.send(course_data)

            class_name = s.recv(2048)
            if class_name == '目前沒有課程':
                self.say(class_name)
                s.close()

                self.onUnload()#it is recommended to reuse the clean-up as the box is stopped
                self.error()
            else:
                s.close()
                self.onStopped(class_name)

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
