        self.words = word_data
        self.logger.info(len(self.words))
        self.run = True

        self.start()

    def onInput_repeat(self, command):
        if command == 'yes':
            self.start()
        else:
            #self.say('You have completed the word practice', self.speech_rate)
            #self.say('You can touch my front head to start reading exercises.', self.speech_rate)
            self.onStopped()

    def onInput_start_practice(self):
        for word in self.words:
            self.say(word, self.speech_rate)
            time.sleep(3)
            if not self.run:
                break
            else:
                pass

        self.say("End of practice, If you want to start again, please say yes, if you don't, say no.", self.speech_rate)
        self.end_of_practice()

    def say(self, p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            configuration =\
            {"bodyLanguageMode":"contextual"}
            id = self.animSpeech.post.say(str(sentence),configuration)
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass

    def onInput_onStop(self):
        self.say('Leave reading', self.speech_rate)
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
