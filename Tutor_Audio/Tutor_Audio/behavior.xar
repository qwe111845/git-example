        self.stu_reading_log['unit'] = p[1]
        try:
            s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            s.connect(('140.134.26.200', port))
            s.send('quiz')
            response = s.recv(2048)
            s.send(str(p[1]))
            time.sleep(0.5)
            data = s.recv(4096)
            self.quiz_data = json.loads(data)
            self.logger.info(self.quiz_data)
        except:
            self.say('伺服器有問題，請檢查伺服器')
        finally:
            try:
                s.close()
                self.startReading_test()
            except:
                pass

    def startReading_test(self):
        self.say('Question ' + str(self.quiz_data['order'][self.order]), self.speed)
        self.say(str(self.quiz_data['quiz'][self.order]), self.speed)
        time.sleep(1)
        self.say(str(self.quiz_data['quiz'][self.order]), self.speed)
        time.sleep(1.5)
        self.say('Which answer do you select?', self.speed)
        self.choose()

    def onInput_get_answer(self, answer):
        self.answer = answer
        self.say('You choose, ' + str(answer) + '!', self.speed)
        time.sleep(1)
        self.say('Are you sure? , you can say yes, or no', self.speed)
        time.sleep(1)
        self.check()

    def onInput_check_answer(self, check):
        if check == 'yes':
            self.stu_reading_log['stu_answer'].append(self.answer)
            self.say('OK! Please touch my front head and say your answer', self.speed)
            self.record_answer()
        elif check == 'no':
            self.say('Which answer do you select?', self.speed)
            self.choose()


    def onInput_student_answer(self, stu_answer):
        self.stu_answer = stu_answer[0]
        word_len = len(self.stu_answer.split(' '))
        #self.logger.info(self.stu_answer)
        self.time = stu_answer[1]
        #self.logger.info(self.time)
        self.reading_speed =  int((word_len / (1.0 * self.time)) * 60.0)
        #self.logger.info(self.speed)
        #self.logger.info(str(self.quiz_data['content'][self.order]))
        truth = str(self.quiz_data['content'][self.order])
        wer = self.get_wer(truth, self.stu_answer)
        #self.logger.info(self.error_rate)

        self.stu_reading_log['stu_read_ans'].append(self.stu_answer)
        self.stu_reading_log['reading_speed'].append(self.reading_speed)
        self.stu_reading_log['wer'].append(wer)

        self.say('Your answer is ' + self.stu_answer, self.speed)
        time.sleep(1)
        #' + self.error_rate + '
        self.say('The correction rate of this conversion is ' + str(wer) + ' percent, and the speed is '+ str(self.speed) + ' words per menit.', self.speed)



        if self.answer == self.quiz_data['answer'][self.order]:
            self.say('You are right! Thank you for your answer.', self.speed)
        else:
            self.say('You are wrong! Thank you for your answer.', self.speed)

        if self.order < 3:
            self.order +=1
            self.say('Next question', self.speed)
            self.startReading_test()
        else:
            self.say('The test is over', self.speed)
            self.sendlog()
            self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
            self.finish_Reading_test()

    def sendlog(self):
        import json
        port = 5007

        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.connect(('140.134.26.200', port))
        s.send('quiz_log')
        response = s.recv(2048)
        time.sleep(0.5)
        self.logger.info(self.stu_reading_log)
        log = json.dumps(self.stu_reading_log)
        s.send(log)

        ans = s.recv(1024)
        self.say(ans)

    def get_wer(self, real, say):
        port = 5007
        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.connect(('140.134.26.200', port))
        s.send('wer')
        response = s.recv(2048)
        s.send(str(real) + ';;' + say)
        time.sleep(0.5)
        wer = s.recv(4096)
        s.close()

        return wer

    def say(self,p, RSPD = 100 , VCT = 100):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( RSPD ) + "\ "
            sentence += "\VCT="+ str( VCT ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.animSpeech.post.say(str(sentence))
            self.ids.append(id)
            self.animSpeech.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False
        pass


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.finish_Reading_test()]]>
